    "typescript",
    "url"
  ],
  "author": "Lonny Gomes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LonnyGomes/urly/issues"
  },
  "homepage": "https://github.com/LonnyGomes/urly#readme",
  "dependencies": {
    "@koa/router": "^9.0.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "koa": "^2.12.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^5.2.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^14.0.13",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.10",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }

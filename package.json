  "description": "A koa-based URL shortener",
  "main": "lib/index.js",
  "scripts": {
    "start": "node ./lib/index.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
